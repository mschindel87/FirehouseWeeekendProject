<h2 class= "custom-pic-header">Pics Throughout the Day...</h2>
		<% @pics.each do |pic| %>
		<div class= "span3">
			<div class="boxy">
				<h4>Hour: </h4><%= pic.hour %> 
				<p/>
				<h4></h4><%= image_tag pic.image %>
				<p/>
				<h4>Emotion: </h4> <%= pic.emotion %> 
				<p />
				<h4>Learned: </h4> <%= pic.learned %> 
				<p />
				
				

			</div>
		</div>

	<% end %>
	<br />





